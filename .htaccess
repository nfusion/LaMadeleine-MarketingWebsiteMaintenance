RewriteRule ^js/(.*) /wp-content/themes/lamadeleine/js/$1 [QSA,L]
RewriteRule ^javascripts/(.*) /wp-content/themes/lamadeleine/js/$1 [QSA,L]
RewriteRule ^img/(.*) /wp-content/themes/lamadeleine/img/$1 [QSA,L]
RewriteRule ^images/(.*) /wp-content/themes/lamadeleine/img/$1 [QSA,L]
RewriteRule ^swf/(.*) /wp-content/themes/lamadeleine/swf/$1 [QSA,L]
RewriteRule ^plugins/(.*) /wp-content/plugins/$1 [QSA,L]
